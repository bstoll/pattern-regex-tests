module regexp-test {
  prefix "rt";
  namespace "urn:openconfig-test";

  import python-plugin-test { prefix "ppt"; }
  import openconfig-bgp-types { prefix "ocbgpt"; }
  import openconfig-inet-types { prefix "ocinet"; }

  leaf ipv4-address {
    type ocinet:ipv4-address;
    ppt:pattern-test-pass "255.255.255.255";
    ppt:pattern-test-pass "0.0.0.0";
    ppt:pattern-test-pass "1.1.1.1";
    ppt:pattern-test-fail "256.255.255.255";
    ppt:pattern-test-fail "1.1.1.256";
    ppt:pattern-test-fail "256.1.1.1%eth0";
  }
  leaf ip-address {
    type ocinet:ip-address;
    ppt:pattern-test-pass "255.255.255.255";
    ppt:pattern-test-pass "2001:db8::1";
    ppt:pattern-test-fail "invalid-data";
    ppt:pattern-test-pass "::1";
  }
  leaf bgp-std-community {
    type ocbgpt:bgp-std-community-type;
    ppt:pattern-test-pass "15169:42";
    ppt:pattern-test-pass "6643:21438";
    ppt:pattern-test-pass "29636:4444";
    ppt:pattern-test-pass "65535:65535";
    ppt:pattern-test-pass "0:0";
    ppt:pattern-test-fail "65536:1";
    ppt:pattern-test-fail "1:65536";
    ppt:pattern-test-fail "425353:comm";
  }
  leaf bgp-ext-community {
    type ocbgpt:bgp-ext-community-type;
    // Type 1 extended communities (2b AS: 4b integer)
    ppt:pattern-test-pass "29636:10";
    ppt:pattern-test-pass "5413:4294967295";
    ppt:pattern-test-pass "4445:0";
    ppt:pattern-test-fail "1273:4294967296";
    ppt:pattern-test-pass "2856:400";
    ppt:pattern-test-fail "5400:invalid";
    ppt:pattern-test-fail "i6643:10";
    ppt:pattern-test-pass "15169:22432";
    // Type 2 extended communities: (4b IP: 2b integer)
    ppt:pattern-test-fail "1.1.1.1:4294967296";
    ppt:pattern-test-fail "1.2.3.4.5:10";
    ppt:pattern-test-pass "82.42.12.35:65535";
    ppt:pattern-test-fail "82.42.12.35:66536";
    ppt:pattern-test-fail "254.254.256.254:10";
    ppt:pattern-test-pass "0.0.0.0:200";
    ppt:pattern-test-fail "leading192.0.2.1:65535";
    // 4b AS : 2b integer
    ppt:pattern-test-fail "4294967296:65535";
    ppt:pattern-test-pass "4294967295:65535";
    ppt:pattern-test-pass "0:65535";
    ppt:pattern-test-pass "4294967295:0";
    ppt:pattern-test-fail "4294967296:0";
    // Route Target Type 1 - route-target:<2b AS>:<4b local>
    ppt:pattern-test-fail "route-target:64";
    ppt:pattern-test-pass "route-target:65535:10";
    ppt:pattern-test-fail "route-TARGET:65535:10";
    ppt:pattern-test-fail "route-target:15169:4294967296";
    ppt:pattern-test-pass "route-target:15169:4294967295";
    // Route Target Type 2 - route-target:<ipv4>:<2b local>
    ppt:pattern-test-fail "route-target:256.0.2.36:10";
    ppt:pattern-test-pass "route-target:192.0.2.1:10";
    ppt:pattern-test-fail "route-target:192.0.2.1:65536";
    // Route Target w/ 4B AS:<2b local>
    ppt:pattern-test-pass "route-target:4294967295:10";
    ppt:pattern-test-fail "route-target:4294967296:10";
    ppt:pattern-test-pass "route-target:5413:65535";
    // Route Origin Type 1 - route-target:<2b AS>:<4b local>
    ppt:pattern-test-fail "route-origin:53";
    ppt:pattern-test-pass "route-origin:65535:10";
    ppt:pattern-test-fail "route-ORIGINTRAIL:65535:10";
    ppt:pattern-test-fail "route-origin:15169:4294967296";
    ppt:pattern-test-pass "route-origin:15169:4294967295";
    // Route Origin Type 2 - route-target:<ipv4>:<2b local>
    ppt:pattern-test-fail "route-origin:512.0.2.36:10";
    ppt:pattern-test-pass "route-origin:10.18.253.24:10";
    ppt:pattern-test-fail "route-origin:192.168.1.1:65536";
    // Route Origin w/ 4B AS:<2b local>
    ppt:pattern-test-pass "route-origin:4294967295:5353";
    ppt:pattern-test-fail "route-origin:4294967296:9009";
    ppt:pattern-test-pass "route-origin:5413:65535";
  }
  leaf ipv6-address {
    type ocinet:ipv6-address;
    ppt:pattern-test-pass "2620::1000:3202:23e:e1ff:fec7:7112";
    ppt:pattern-test-pass "fe80::23e:e1ff:fec7:7112";
    ppt:pattern-test-fail "fe80::23e:NOTVALID:fec7:7112";
    ppt:pattern-test-fail "FFFF::NOTE::FFFF";
    ppt:pattern-test-fail "FFFF::1::42";
  }
  leaf ipv4-prefix {
    type ocinet:ipv4-prefix;
    ppt:pattern-test-pass "0.0.0.0/0";
    ppt:pattern-test-pass "255.255.255.255/32";
    ppt:pattern-test-fail "256.0.0.0/31";
    ppt:pattern-test-pass "1.2.3.0/24";
    ppt:pattern-test-fail "1.2.3.4/33";
  }
  leaf ipv6-prefix {
    type ocinet:ipv6-prefix;
    ppt:pattern-test-pass "::/0";
    ppt:pattern-test-pass "FFFF:FFFF:FFFF:FFFF:FFFF:FFFF:FFFF:FFFF/128";
    ppt:pattern-test-fail "FFFF:FFFF:FFFF:NOTVALID:FFFF:FFFF:FFFF:FFFF/64";
    ppt:pattern-test-pass "2001:DB8::/32";
    ppt:pattern-test-fail "2001:4C20::/129";
  }
  leaf ip-prefix {
    type ocinet:ip-prefix;
    ppt:pattern-test-pass "0.0.0.0/0";
    ppt:pattern-test-pass "192.0.2.1/32";
    ppt:pattern-test-fail "192.0.2.2/33";
    ppt:pattern-test-pass "FE80::CAFE/128";
    ppt:pattern-test-fail "FE81::CAFE:DEAD:BEEF/129";
  }
  leaf domain-name {
    type ocinet:domain-name;
    ppt:pattern-test-pass "ambroseesorbma";
    ppt:pattern-test-fail "ambrose esorbma";
    ppt:pattern-test-pass "claire.";
    ppt:pattern-test-fail "~~~-+=.";
    ppt:pattern-test-fail "ambrose esorbma";
    ppt:pattern-test-pass "012345678901234567890123456789012345678901234567890123456789012.";
    ppt:pattern-test-fail "0123456789012345678901234567890123456789012345678901234567890123";
  }
}
